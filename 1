#include<stdio.h>
#include<stdlib.h>
int main() {
	while (1) {
		int n;
		printf("n=");
		scanf_s("%d", &n);
		int* a = (int*)malloc(n * sizeof(int*));
		for (int i = 0; i < n; i++) {
			scanf_s("%d", &a[i]);
		}
		for (int i = 0; i < n; i++) {
			if (i == n - 1) {
				printf("%d\n",a[i]);
			}
			else {
			if(*a!=NULL)
			    printf("%d ", a[i]);
				}
		}
	}
	return 0;
}
